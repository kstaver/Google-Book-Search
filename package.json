{
  "name": "google-book-search",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir dist",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kstaver/Google-Book-Search.git"
  },
  "author": "Kendra Staver",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kstaver/Google-Book-Search/issues"
  },
  "homepage": "https://github.com/kstaver/Google-Book-Search#readme",
  "dependencies": {
    "apollo-server": "^3.6.1",
    "bcrypt": "^5.0.1",
    "concurrently": "^7.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "mongodb": "^4.2.2",
    "node": "^17.3.0",
    "package-lock": "^1.0.5",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "reactdom": "^2.0.0"
  }
}
